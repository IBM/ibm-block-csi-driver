pipeline {
    agent {
        label 'ansible_rhel73'
    }
    stages {
        stage ('Run CSI controller unittests (inside container)') {
            steps {
                sh './scripts/run_unittest.sh'
            }
        }
        stage ('Build CSI controller image') {
            steps {
                sh './scripts/build_imag.sh'
            }
        }
	}
}
